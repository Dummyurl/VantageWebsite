    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/lounch_icon"
        android:label="@string/app_name"
        android:theme="@style/custom_title_theme" >
        <activity
            android:name="co.vantagewebtech.Tabs"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="co.vantagewebtech.HomePage"
            android:label="@string/title_activity_home_page" >
        </activity>
        <activity
            android:name="co.vantagewebtech.TabStackTemple"
            android:label="@string/title_activity_home_page" >
        </activity>
        <activity
            android:name="co.vantagewebtech.ServicesPage"
            android:label="@string/title_activity_services_page" >
        </activity>
        <activity
            android:name="co.vantagewebtech.AboutUsPage"
            android:label="@string/title_activity_about_us_page" >
        </activity>
        <activity
            android:name="co.vantagewebtech.PortFolioPage"
            android:label="@string/title_activity_port_folio_page" >
        </activity>
        <activity
            android:name="co.vantagewebtech.ContactUsPage"
            android:label="@string/title_activity_contact_us_page" >
        </activity>
        <activity
            android:name="co.vantagewebtech.WebViewActivity"
            android:label="@string/title_activity_web_view" >
        </activity>
    </application>

</manifest>
